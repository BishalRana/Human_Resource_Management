@model Human_Resource_Management.Models.Project

@{
    ViewData["Title"] = "Details";
}
<div class="row">
    <div class="page-header">
        <h2>Project Information</h2>
    </div>
</div>
<div class="row">
    <div class="col-sm-11">
        <dl class="dl-horizontal">
            <dt>
                Project 
            </dt>
            <dd>
                @Html.DisplayFor(model => model.Name)
            </dd>
            <dt>
                Sub Company 
            </dt>
            <dd>
                @Html.DisplayFor(model => model.SubCompany.Name)
            </dd>

        </dl>
    </div>
</div>
<div class="row" style="padding-top:20px;">
    <div class="col-sm-offset-1 col-sm-11" style="padding-top:10px;">
        @Html.ActionLink("Edit Record", "Edit", new { id = Model._PjtId }) |
        @Html.ActionLink("Back to Project List", "Index")
    </div>
</div>
